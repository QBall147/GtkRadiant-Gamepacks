//asylum
textures/proto2/asylum_floor_w
{
	{
		map textures/base_wall/chrome_env.tga
		tcgen environment
		rgbgen wave sin .25 0 0 0
	}


	{
		map textures/proto2/asylum_floor_w.tga
		blendfunc GL_ZERO GL_SRC_ALPHA
		tcmod scale .1 .1
		rgbgen identity	
	}

	{
		map textures/proto2/asylum_floor_w.tga
		blendfunc GL_ONE GL_SRC_ALPHA
		rgbgen identity	
	}


	{
		map $lightmap
        	blendfunc filter
		rgbgen identity 
	}

}
